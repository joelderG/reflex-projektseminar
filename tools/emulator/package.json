{
  "name": "@reflex/emulator",
  "author": "Mathias Müller",
  "description": "Emulator for depth sensing input for developing applications for Elastic Displays using the ReFlex framework",
  "version": "0.9.8",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4300",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build --base-href ./ && electron .",
    "build:electron-win": "ng build --base-href ./ && electron-builder build --win"
  },
  "build": {
    "appId": "reflex.emulator",
    "productName": "ReFlex Emulator",
    "copyright": "Copyright © 2023",
    "buildVersion": "0.9.8",
    "electronVersion": "23.2.4",
    "win": {
      "icon": "dist/assets/icon/app_icon_inverted.ico"
    },
    "files": [
      "node_modules/**/*",
      "**/*"
    ],
    "directories": {
      "output": "release",
      "buildResources": "assets"
    }
  },
  "private": true,
  "dependencies": {
    "@fortawesome/angular-fontawesome": "^0.13.0",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",    
    "@reflex/shared-types": "file:../../packages/reflex-shared-types",
    "bootstrap": "^5.3.0",
    "@angular/animations": "16.1.6",
    "@angular/common": "16.1.6",
    "@angular/compiler": "16.1.6",
    "@angular/core": "16.1.6",
    "@angular/forms": "16.1.6",
    "@angular/platform-browser": "16.1.6",
    "@angular/platform-browser-dynamic": "16.1.6",
    "@angular/platform-server": "16.1.6",
    "@angular/router": "16.1.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.5",
    "@angular/cli": "16.1.5",
    "@angular/compiler-cli": "16.1.6",
    "@angular/language-service": "16.1.6",
    "@gtvmbh/eslint-config": "^1.0.8",
    "@nguniversal/common": "^16.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.20.55",
    "@types/ws": "<8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.45.0",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^4.1.0",
    "eslint-plugin-jsdoc": "^40.3.0",
    "jasmine-core": "~4.5.0",
    "jasmine-marbles": "^0.8.4",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-spec-reporter": "^0.0.36",
    "karma-viewport": "^1.0.9",
    "puppeteer": "^19.11.1",
    "typescript": "^5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/visualengineers/reflex.git"
  },
  "keywords": [
    "elastic-displays",
    "shape-changing-interfaces",
    "electron",
    "developer-tool",
    "emulator"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/visualengineers/reflex/issues"
  },
  "homepage": "https://github.com/visualengineers/reflex#readme",
  "browserslist": [
    "defaults"
  ]
}
